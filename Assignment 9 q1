#include <iostream>
using namespace std;


struct Item {
    int id;     
    float cost;
};

int main() {
    
    Item items[6] = {
        {1, 45.50},  
        {2, 75.30},  
        {3, 100.00},
        {4, 30.00}, 
        {5, 968.97},   
        {0, 0.0}     
    };

    
    for (int i = 0; i < 6; i++) {
        if (items[i].cost > 50.00) {
            cout << "Item ID: " << items[i].id << ", Cost: $" << items[i].cost << endl;
        }
    }

    return 0;
}
